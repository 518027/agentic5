@prefix : <http://www.w3id.org/agentic-ai/onto#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

ex:Pattern1 a :WorkflowPattern ;
  dcterms:title "Literature Review Pipeline" ;
  :hasWorkflowStep ex:Step1 , ex:Step2 .

ex:Step1 a :WorkflowStep ;
  :stepOrder 1 ;
  :performedBy ex:AgentA ;
  :hasAssociatedTask ex:SearchLiterature .

ex:Step2 a :WorkflowStep ;
  :stepOrder 2 ;
  :performedBy ex:AgentA ;
  :hasAssociatedTask ex:Summarize .

ex:AgentA a :Agent ;
  :usesTool ex:SearchAPI .

# second pattern
ex:Pattern2 a :WorkflowPattern ;
  dcterms:title "Summarization with Human in Loop" ;
  :hasWorkflowStep ex:P2Step1 , ex:P2Step2 .

ex:P2Step1 a :WorkflowStep ;
  :stepOrder 1 ;
  :performedBy ex:AgentB ;
  :hasAssociatedTask ex:GenerateSummary .

ex:P2Step2 a :WorkflowStep ;
  :stepOrder 2 ;
  :performedBy ex:HumanReviewer ;
  :hasAssociatedTask ex:ApproveSummary .
